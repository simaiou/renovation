.main{
	padding-bottom: 20rpx;
}
.bgc {
	width: 100%;
	height: 400rpx;
	padding-top: 80rpx;
	background-color: #1cbbb4;

}
.container {
	z-index: 10000 !important;
	height: 160rpx;
}

.my{
	margin-bottom:80rpx;
}
.container .card {
	padding: 0 30rpx;
	display: flex;
	flex-direction: column;
	border-radius: 10rpx;
	height: 40%;
	background-color: #fff;
}

.card .user-info {
	display: flex;
	align-items: center;
	flex-direction: column;
	justify-content: center;
}

.user-info .user {
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.user .name{
	font-size: 40rpx;
	color: #fff;
}

.card image {
	width: 120rpx;
	height: 120rpx;
	margin-bottom: 20rpx;
	border-radius: 50%;
}

.score-info {
	width: 100%;
	display: flex;
	position: absolute;
	justify-content: space-around;
	z-index: 1000;
}

.score-info .number {
	color: rgb(255,255,255);
	font-size: 40rpx;
}

.score-info text {
	color: #fff;
	font-size: 24rpx;
}

.score-info .score,
.score-info .raking {
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;

}

/* cell样式 */
i-cell-group .height {
	line-height: 2;
	color: #666;
}

/*组件背景颜色*/
.zr {
	border-radius: 8rpx;
	width: 100%;
	height: 160rpx;
	position: relative;
}

.water {
	position: absolute;
	left: 0;
	height: 30px;
	bottom: 0;
	width: 100%;
	z-index: 1;
}


.water-c {
	position: relative;
	}

.water-1 {
	top: 20px;
	background: url("data:image/svg+xml;base64,") repeat-x;
	background-size: 600px;
	-webkit-animation: wave-animation-1 3s infinite linear;
	animation: wave-animation-1 3s infinite linear
}

.water-2 {
top: 30px;
	background: url("data:image/svg+xml;base64,") repeat-x;
	background-size: 600px;
	-webkit-animation: wave-animation-2 6s infinite linear;
	animation: wave-animation-2 6s infinite linear
}

.water-1,
.water-2 {
	position: absolute;
	width: 100%;
	height: 60px;
}

.back-white {
	background: #fff
}

@keyframes wave-animation-1 {
	0% {
		background-position: 0 top
	}

	100% {
		background-position: 600px top
	}
}

@keyframes wave-animation-2 {
	0% {
		background-position: 0 top
	}

	100% {
		background-position: 600px top
	}
}

